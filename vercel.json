{
  "rewrites": [
    {
      "source": "/moovie",
      "destination": "/moovie-nextjs"
    },
    {
      "source": "/moovie/:path*",
      "destination": "/moovie-nextjs/:path*"
    },
    {
      "source": "/:shortId([a-zA-Z0-9]+)",
      "destination": "/redirect.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/add-group",
      "destination": "https://www.whatsuplink.online/?submit-form=true",
      "permanent": true
    },
    {
      "source": "/usa-whatsapp-group-links",
      "destination": "https://www.whatsuplink.online/country/us",
      "permanent": true
    },
    {
      "source": "/active-pakistani-whatsapp-group-links",
      "destination": "https://www.whatsuplink.online/country/pk",
      "permanent": true
    },
    {
      "source": "/indian-whatsapp-group-links",
      "destination": "https://www.whatsuplink.online/country/in",
      "permanent": true
    }
  ],
  "builds": [
    {
      "src": "moovie-nextjs/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/moovie/(.*)",
      "dest": "/moovie-nextjs/$1"
    }
  ]
}
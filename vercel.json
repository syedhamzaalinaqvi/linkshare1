{
  "rewrites": [
    { "source": "/:shortId([a-zA-Z0-9]+)", "destination": "/redirect.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=1, stale-while-revalidate" }
      ]
    }
  ],
   "redirects": [
    {
      "source": "/add-group",
      "destination": "https://www.whatsuplink.online/?submit-form=true",
      "permanent": true
    }
  ]
}
